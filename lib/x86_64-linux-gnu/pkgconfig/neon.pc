prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include

Name: neon
Description: HTTP/WebDAV client library
Version: 0.30.2
Libs: -L${libdir} -lneon 
Libs.private:  -lz -lssl -lcrypto -L/usr/lib/x86_64-linux-gnu/mit-krb5 -Wl,-Bsymbolic-functions -Wl,-z,relro -lgssapi_krb5 -lkrb5 -lk5crypto -lcom_err -lxml2
Cflags: -I${includedir}/neon 
