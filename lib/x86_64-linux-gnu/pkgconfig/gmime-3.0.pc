prefix=/usr
exec_prefix=${prefix}
libdir=${prefix}/lib/x86_64-linux-gnu
includedir=${prefix}/include


Name: GMime
Version: 3.2.7
Description: MIME parser and utility library
Requires: glib-2.0,gio-2.0,gobject-2.0
Libs: -L${libdir} -lgmime-3.0
Libs.private: -lz -lnsl -L/usr/lib/x86_64-linux-gnu -lgpgme -lassuan -lgpg-error
Cflags: -I${includedir}/gmime-3.0 -D_LARGEFILE64_SOURCE  
